# home-brew doctor recommeds
export PATH="/usr/local/sbin:$PATH"

# defaut editor
export EDITOR='nvim'

# emacs key bindings
bindkey -e

# aliases
alias l='ls -lahFGT'

# ls colors
export LSCOLORS=exdxxxxxbxbxxxxxxxxxxx

# pager settings
export LESS="--quit-if-one-screen --no-init --IGNORE-CASE --RAW-CONTROL-CHARS"
